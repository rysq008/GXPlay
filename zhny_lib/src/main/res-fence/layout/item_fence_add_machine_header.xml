<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <import type="android.view.View"/>
        <variable
            name="name"
            type="java.lang.String" />
    </data>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_87"
        tools:background="#CCCCCC"
        >

        <TextView
            android:id="@+id/item_fence_add_machine_header_rectangle"
            android:layout_width="@dimen/dp_4"
            android:layout_height="@dimen/dp_31"
            android:layout_marginStart="@dimen/dp_30"
            android:background="@drawable/shape_green_rectangle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <TextView
            android:id="@+id/type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_10"
            android:textColor="@color/color_009688"
            android:textSize="@dimen/sp_28"
            app:layout_constraintLeft_toRightOf="@id/item_fence_add_machine_header_rectangle"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            textStrNotNull="@{name}"
            tools:text="撒肥车"
            />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>