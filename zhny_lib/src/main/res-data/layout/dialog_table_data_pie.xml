<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <import type="android.view.View"/>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="@dimen/dp_690"
        android:layout_height="@dimen/dp_690"
        tools:background="#cccccc">

        <View
            android:id="@+id/v_dialog_table_data_pie"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_2"
            android:layout_marginTop="@dimen/dp_103"
            android:background="#FFF4F4F4"
            app:layout_constraintTop_toTopOf="parent"
            />

        <TextView
            android:id="@+id/tv_dialog_table_data_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/v_dialog_table_data_pie"
            android:textColor="#009688"
            android:textSize="@dimen/sp_36"
            tools:text="测试名称"
            />

        <RelativeLayout
            android:id="@+id/rl_close_dialog_table_data_pie"
            android:layout_width="@dimen/dp_100"
            android:layout_height="@dimen/dp_100"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/v_dialog_table_data_pie"
            >
            <ImageView
                android:layout_width="@dimen/dp_35"
                android:layout_height="@dimen/dp_35"
                android:layout_centerInParent="true"
                android:src="@drawable/icon_close"
                tools:ignore="ContentDescription" />
        </RelativeLayout>


        <com.github.mikephil.charting.charts.PieChart
            android:id="@+id/pc_dialog_table_data"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_514"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="@+id/v_dialog_table_data_pie"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>