<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="wrap_content"    android:layout_height="wrap_content"    android:background="@drawable/shape_white_solid_corner10101010_rect"    android:orientation="vertical"    android:paddingLeft="@dimen/dp_15"    android:paddingRight="@dimen/dp_15">    <TextView        android:id="@+id/dialog_title_tv"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="@dimen/dp_24"        android:text="已将验证码发到尾号****手机"        android:textColor="@color/C_333333"        android:textSize="@dimen/dp_16"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <EditText             android:textCursorDrawable="@drawable/shape_black_solid_width_1_rect"        android:id="@+id/dialog_input_et"        android:layout_width="@dimen/dp_249"        android:layout_height="@dimen/dp_48"        android:layout_marginTop="@dimen/dp_22"        android:background="@drawable/shape_grey_stroke_6_radius_rect"        android:gravity="center_vertical"        android:paddingLeft="@dimen/dp_5"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/dialog_title_tv" />    <CheckBox        android:id="@+id/dialog_eye_cb"        android:layout_width="@dimen/dp_24"        android:layout_height="@dimen/dp_24"        android:background="@drawable/selector_open_close_eye"        android:button="@null"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="@+id/dialog_input_et"        app:layout_constraintEnd_toEndOf="@+id/dialog_input_et"        app:layout_constraintTop_toTopOf="@+id/dialog_input_et"        tools:visibility="visible" />    <Button        android:id="@+id/dialog_ok_btn"        android:layout_width="@dimen/dp_79"        android:layout_height="@dimen/dp_32"        android:layout_marginTop="@dimen/dp_32"        android:layout_marginBottom="@dimen/dp_24"        android:background="@drawable/shape_gradient_line_180_green2blue_rect"        android:text="下一步"        android:textColor="@android:color/white"        android:textSize="@dimen/dp_14"        app:layout_constraintBottom_toTopOf="@+id/dialog_tips_tv"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/dialog_input_et" />    <TextView        android:id="@+id/dialog_resend_tv"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="30S重发"        android:textColor="@color/color_999"        android:textSize="@dimen/dp_12"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="@id/dialog_ok_btn"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toTopOf="@id/dialog_ok_btn"        tools:visibility="visible" />    <TextView        android:id="@+id/dialog_tips_tv"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="@dimen/dp_8"        android:layout_marginBottom="@dimen/dp_32"        android:text="遇到问题？\n手机号不可用"        android:textColor="@color/color_666666"        android:textSize="@dimen/dp_12"        android:visibility="gone"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/dialog_ok_btn"        tools:visibility="visible" /></androidx.constraintlayout.widget.ConstraintLayout>